      {
        "command": "makefile.outline.postConfigure",
        "title": "%makefile-tools.command.makefile.postConfigure.title%"
      },
      {
        "command": "makefile.resetState",
        "title": "%makefile-tools.command.makefile.resetState.title%"
      }
