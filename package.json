    "viewsContainers": {
      "activitybar": [
        {
          "id": "makefile__viewContainer",
          "title": "Makefile",
          "when": "makefile:fullFeatureSet",
          "icon": "res/viewcontainer.svg"
        }
      ]
    },
    "views": {
